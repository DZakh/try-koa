{
  "name": "try-koa",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc --build",
    "start": "ts-node-dev src/server.ts",
    "test": "cross-env NODE_ENV=test PORT=7788 jest",
    "test:watch": "npm run test -- --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DZakh/try-koa.git"
  },
  "author": "Dmitry Zakharov <dzakh.dev@gmail.com> (https://dzakh.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DZakh/try-koa/issues"
  },
  "homepage": "https://github.com/DZakh/try-koa#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^3.1.0",
    "koa-router": "^7.3.0",
    "koa2-cors": "^2.0.5",
    "lodash": "^4.17.19",
    "node-fetch": "^2.6.0",
    "redis": "^2.8.0",
    "ts-node": "^3.3.0",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/koa": "^2.0.49",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.0.42",
    "@types/koa2-cors": "^2.0.1",
    "@types/lodash": "^4.14.155",
    "@types/node": "^10.14.12",
    "@types/node-fetch": "^2.5.7",
    "@types/redis": "^2.8.13",
    "@types/supertest": "^2.0.8",
    "cross-env": "^7.0.2",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "prettier": "1.15.1",
    "pretty-quick": "^1.11.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.0.0-pre.40"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "roots": [
      "<rootDir>/__tests__"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
